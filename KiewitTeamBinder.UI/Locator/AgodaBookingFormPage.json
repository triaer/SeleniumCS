{
  "default": {
    "_lblHotelName": {
      "type": "xpath",
      "value": "//span[@class='hotel-name']"
    },
    "_lblBookingDateRange": {
      "type": "xpath",
      "value": "//*[@data-bind='html: checkinCheckoutDetails.checkinCheckoutDate']"
    },
    "_lblDuration": {
      "type": "xpath",
      "value": "//*[@data-bind='text: checkinCheckoutDetails.nightsText']"
    },
    "_lblRoomTypeList": {
      "type": "xpath",
      "value": "//*[@data-bind='text: roomDetails.roomTypeListText']"
    },
    "_lblRoomOccupancy": {
      "type": "xpath",
      "value": "//*[@data-bind='text: roomDetails.occupancy']"
    },
    "_lblBasePrice": {
      "type": "xpath",
      "value": "//*[@data-bind='text: basePrice.formattedAmount']"
    }
  }
}